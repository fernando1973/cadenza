{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
    "packages": [
      "nodejs_20",
      "yarn",
      "git",
      "python3",
      "gcc",
      "gh"
    ],
    "shell": {
      "init_hook": [
        "echo 'Welcome to Cadenza devbox!' > /dev/null"
      ],
      "scripts": {
        "test": [
          "echo \"Error: no test specified\" && exit 1"
        ],
        "start": [
          "yarn start"
        ],
        "install": [
          "yarn install"
        ]
      }
    }
  }
  
